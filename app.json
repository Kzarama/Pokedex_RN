{
  "expo": {
    "name": "Pokedex",
    "slug": "Pokedex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pokeball-dark.png",
    "scheme": "pokedex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kzarama.Pokedex"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pokeball-dark.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kzarama.Pokedex"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/pokeball-dark.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/pokeball-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "33ddbe63-cef6-44f6-9677-e78429082340"
      }
    }
  }
}
